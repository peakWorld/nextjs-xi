{
  "type": "root",
  "nodes": [
    {
      "type": "atrule",
      "name": "import",
      "params": "url('./var.less')",
      "import": true,
      "filename": "'./var.less'"
    },
    {
      "type": "atrule",
      "name": "width",
      "params": "10px",
      "variable": true,
      "value": "10px"
    },
    {
      "type": "atrule",
      "name": "height",
      "params": "@width + 10px",
      "variable": true,
      "value": "@width + 10px"
    },
    {
      "type": "atrule",
      "name": "my-selector",
      "params": "banner",
      "variable": true,
      "value": "banner"
    },
    {
      "type": "atrule",
      "name": "images",
      "params": "\"../img\"",
      "variable": true,
      "value": "\"../img\""
    },
    {
      "type": "atrule",
      "name": "property",
      "params": "color",
      "variable": true,
      "value": "color"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "height",
          "value": "@height"
        }
      ],
      "selector": "body"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "@{property}",
          "value": "#0ee"
        },
        {
          "type": "decl",
          "prop": "font-weight",
          "value": "bold"
        },
        {
          "type": "decl",
          "prop": "background",
          "value": "url(\"@{images}/white-sand.png\")"
        }
      ],
      "selector": ".@{my-selector}"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "color",
          "value": "red"
        },
        {
          "type": "decl",
          "prop": "background-color",
          "value": "$color"
        },
        {
          "type": "decl",
          "prop": "padding",
          "value": "20px"
        },
        {
          "type": "rule",
          "nodes": [
            {
              "type": "decl",
              "prop": "margin",
              "value": "$padding"
            }
          ],
          "selector": ".inner"
        }
      ],
      "selector": ".block"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "color",
          "value": "blue"
        },
        {
          "type": "rule",
          "nodes": [
            {
              "type": "decl",
              "prop": "color",
              "value": "green"
            }
          ],
          "selector": "&:hover"
        },
        {
          "type": "rule",
          "nodes": [
            {
              "type": "decl",
              "prop": "color",
              "value": "green"
            }
          ],
          "selector": "& &"
        }
      ],
      "selector": "a.text"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "color",
          "value": "red"
        }
      ],
      "selector": ".inline"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "&",
          "value": "extend(.inline)",
          "extend": true
        },
        {
          "type": "decl",
          "prop": "background",
          "value": "blue"
        }
      ],
      "selector": "nav ul"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "background",
          "value": "green"
        }
      ],
      "selector": "nav ul:extend(.inline)",
      "extend": true
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "box-shadow+",
          "value": "inset 0 0 10px #555"
        },
        {
          "type": "decl",
          "prop": "transform+_",
          "value": "scale(2)"
        }
      ],
      "selector": ".mixin()"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "atrule",
          "name": "mixin",
          "params": "()",
          "mixin": true
        },
        {
          "type": "decl",
          "prop": "box-shadow+",
          "value": "0 0 20px black"
        },
        {
          "type": "decl",
          "prop": "transform+_",
          "value": "rotate(15deg)"
        }
      ],
      "selector": ".myclass"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "color",
          "value": "red"
        }
      ],
      "selector": ".a"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "background-color",
          "value": "green"
        }
      ],
      "selector": "#b()"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "atrule",
          "name": "a",
          "params": "()",
          "mixin": true
        },
        {
          "type": "atrule",
          "name": "b",
          "params": "()",
          "mixin": true
        }
      ],
      "selector": ".mixin-class"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "background",
          "value": "@bg"
        },
        {
          "type": "decl",
          "prop": "color",
          "value": "@color"
        }
      ],
      "selector": ".mixin (@bg; @color: #900)"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "atrule",
          "name": "mixin",
          "params": "(#f5f5f5 no-repeat, green)",
          "mixin": true,
          "important": true
        }
      ],
      "selector": ".class1"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "atrule",
          "name": "mixin",
          "params": "(@color: #efca44; @bg: red)",
          "mixin": true
        }
      ],
      "selector": ".class2"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "decl",
          "prop": "box-shadow",
          "value": "@arguments"
        }
      ],
      "selector": ".box-shadow(@x: 0, @y: 0, @blur: 1px, @color: #000)"
    },
    {
      "type": "rule",
      "nodes": [
        {
          "type": "atrule",
          "name": "box-shadow",
          "params": "(2px, 5px)",
          "mixin": true
        }
      ],
      "selector": ".big-block"
    }
  ]
}
